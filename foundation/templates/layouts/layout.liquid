        #Slider {
            cursor: hand
        }
        #Actual {
            background: #fff;
            color: White;
            padding: 0px;
        }
        .slideup, .slidedown {
            max-height: 0;            
            overflow-y: hidden;
            -webkit-transition: max-height 0.8s ease-in-out;
            -moz-transition: max-height 0.8s ease-in-out;
            -o-transition: max-height 0.8s ease-in-out;
            transition: max-height 0.8s ease-in-out;

        }
        .slidedown {            
            max-height: 400px ;                    
        }    

        .slidenav {background:#fff;color:#000;width:100%;padding:6px 6px;margin:0;text-align:left;}
        .slidenav:hover {background:#fff;color:#333;width:100%;padding:6px 6px;margin:0;text-align:left;}
        .slidenav:active {background:#C03;color:#333;width:100%;padding:6px 6px;margin:0;text-align:left;}
        .slidenav:visited {background:#F00;color:#333;width:100%;padding:6px 6px;margin:0;text-align:left;}
               



ul.downloadtop {clear:both;margin:30px 0 5px 0;}
