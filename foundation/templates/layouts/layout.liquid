h3 {font-size:0.7em;text-transform:uppercase;letter-spacing:4px;color:#999;}
h5 {font-size:0.6em;text-transform:uppercase;}*/


/*ID*/
h2.id-header {font-size:0.9em;text-transform:uppercase;letter-spacing:4px;color:#999;margin:0 0 20px 0;}

/* title result*/

.title-result {padding:10px 20px 30px 0px;font-size:1.2em;color:#666;}
.title-result span {color:#f90;font-size:1em;}

ul.resultlist { margin: 0 0px;padding: 0;list-style-type: none;}
.resultlist li { text-transform: capitalize; border-top:1px solid #fff;padding: 0px;}
.resultlist li:first-child { border:0;}
.resultlist li span { font-size:0.7em;color:#999;margin-left:10px;}
.resultlist a { display: block;width: 100%;text-decoration: none;height:40px;padding: 10px 0 0 10px;}

.resultlist a:hover { background-color: #fff;padding: 0px; transition: background .45s ease-in-out; -moz-transition:background .45s ease-in-out; -webkit-transition: background .45s ease-in-out;padding: 10px 0 0 10px;height:40px;}


.divider {border:1px solid #f0f1f3;}


.scopeNote {font-size:0.9em;}
ul.scopeNote  {margin:0;padding:0;}
.scopeNote li {list-style-type:none;list-style-position:outside !important;margin:0;padding:0 0 20px 0;}



ul.subNav  		{list-style-type:none;list-style-position:outside !important;margin:67px 0 0 0;padding:0;}
ul.subNav li 	{background:#fff;margin:10px 0 0 0;padding:10px 14px;font-size:0.8em;text-transform: uppercase;}

ul.subNav2  	{list-style-type:none;list-style-position:outside !important;margin:0;padding:0;}
ul.subNav2 li 	{background:#fff;margin:0;padding:10px 14px;font-size:0.7em;text-transform: capitalize;}



/* mag glass large */
.ico-mglass-large {
  position: relative;
  display: inline-block;
  background: #f9f9f9;
  border-radius: 60px;
  height: 30px;
  width: 30px;
  border: 3px solid #f77f00;margin:10px 0 0 10px;
}
.ico-mglass-large:after {
  content: "";
  height: 4px;
  width: 16px;
  background: #f77f00;
  position: absolute;
  top: 26px;
  left: 18px;
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
}



/* mag glass small */
.ico-mglass {
  position: relative;
  display: inline-block;
  background: transparent;
  border-radius: 60px;
  height: 18px;
  width: 18px;
  border: 2px solid #f77f00;margin:18px 0 0 16px;
}
.ico-mglass:after {
  content: "";
  height: 2px;
  width: 12px;
  background: #f77f00;
  position: absolute;
  top: 16px;
  left: 10px;
  -webkit-transform: rotate(45deg);
  -moz-transform: rotate(45deg);
  -ms-transform: rotate(45deg);
  -o-transform: rotate(45deg);
}



.box {background:#fff;margin-bottom:20px;padding:20px 10px 25px 10px;border:1px solid #efefef;}
.box h5 {font-size:0.7em;font-weight:bold;text-transform:uppercase;letter-spacing: 3px;color:#666;margin-bottom:20px;border-bottom:1px solid #efefef;padding:0 0 20px 0;}
.box ul {font-size:0.8em;}

.box3 {background:#fff;margin:0 0 0px 0;border:1px solid #efefef;text-align:center;height:100px;padding:20px 10px 20px 10px;overflow:hidden;}
.box3 a {display:block;}
.box3 a:hover {display:block;background-color: #efefef;height:100px; }
.box3 h5 {font-size:0.7em;font-weight:bold;text-transform:uppercase;letter-spacing: 3px;padding:10px 0 0px 0;margin:0;}
.box3 span {font-size:0.7em;color:#999;}

.subfooter, .subfooterright {padding:0 20px;border-left:1px solid #ccc;margin-top:20px;font-size:0.8em}
.subfooter h3, .subfooterright h3 {font-size:0.7em;text-transform:uppercase;letter-spacing:4px;color:#999;}
.subfooterright {border-right:1px solid #ccc;}

/*home*/
.language {font-size:0.5em;margin:10px 0 0 0;padding:0}
.language-active {color:#f77f00;}

.bgcontainer {background:#fff;padding:20px 0;border:1px solid #f0f1f3;}

.home-img_position {position:relative}
.home-img_position form {position:absolute;z-index:100;width:100%;margin-top:50px}
.home-img_position form input {background:#f9f9f9;}

.logo {margin:10px 20px 0 -32px;}
h1.logotext  {color:#a5a5a5;letter-spacing:16px;font-size:0.7em;margin:30px 0;text-transform:uppercase}


