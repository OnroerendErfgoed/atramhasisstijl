.sticky {border-bottom:1px solid #f0f1f3;}



	a {color:#2b709d;}
	a:hover {color:#f77f00 !important;}


// list
dl.infolist {font-size:0.7em; font-family: 'ambleregular', sans-serif;}
.infolist dt {margin-left:0px;font-weight:normal;font-size:0.7em; font-family: 'ambleregular', sans-serif;}
.infolist dd {margin:-22px 0 0 70px;font-size:0.8em; font-family: 'amblebold', sans-serif;}

// submenu 
.nostyleList {
   list-style: none;
   margin-left: 0;
   padding-left: 0 }



h1 {font-size:2em;color:#f77f00;margin:20px  0 0 0;}
